
<%= simple_form_for(@empire_customer) do |f| %>
  <%= f.error_notification %>
  <%= f.error_notification message: f.object.errors[:base].to_sentence if f.object.errors[:base].present? %>

  <div class="form-inputs">
    <%= f.input :e_id %>
    <%= f.input :uid %>
    <%= f.input :lic_state %>
    <%= f.input :lic_num %>
    <%= f.input :existing %>
    <%= f.input :purchase_s %>
    <%= f.input :purchase %>
    <%= f.input :price_s %>
    <%= f.input :price %>
    <%= f.input :product %>
    <%= f.input :email %>
    <%= f.input :fname %>
    <%= f.input :lname %>
    <%= f.input :street_1 %>
    <%= f.input :street_2 %>
    <%= f.input :city %>
    <%= f.input :state %>
    <%= f.input :zip %>
  </div>
<br>
  <div class="form-actions">
    <%= f.button :submit, 'Submit', class: 'btn btn-primary btn-gradiant w100' %>
  </div>
  <br>
<% end %>
