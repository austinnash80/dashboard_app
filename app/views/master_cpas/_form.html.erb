
<%= simple_form_for(@master_cpa) do |f| %>
  <%= f.error_notification %>
  <%= f.error_notification message: f.object.errors[:base].to_sentence if f.object.errors[:base].present? %>

  <div class="form-inputs">
    <%= f.input :lid %>
    <%= f.input :list %>
    <%= f.input :lic_st %>
    <%= f.input :lic %>
    <%= f.input :fname %>
    <%= f.input :mi %>
    <%= f.input :lname %>
    <%= f.input :suf %>
    <%= f.input :co %>
    <%= f.input :add %>
    <%= f.input :add2 %>
    <%= f.input :city %>
    <%= f.input :st %>
    <%= f.input :zip %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
