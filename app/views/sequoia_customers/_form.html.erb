
<%= simple_form_for(@sequoia_customer) do |f| %>
  <%= f.error_notification %>
  <%= f.error_notification message: f.object.errors[:base].to_sentence if f.object.errors[:base].present? %>

  <div class="form-inputs">
    <%= f.input :s_id, label: 'S ID' %>
    <%= f.input :order_id, label: 'Order ID' %>
    <%= f.input :uid %>
    <%= f.input :lic_state %>
    <%= f.input :lic_num %>
    <%= f.input :exisiting %>
    <%= f.input :purchase_s %>
    <%= f.input :purchase, include_blank: true %>
    <%= f.input :price_s %>
    <%= f.input :price %>
    <%= f.input :product_1 %>
    <%= f.input :product_2 %>
    <%= f.input :designation %>
    <%= f.input :email %>
    <%= f.input :fname %>
    <%= f.input :lname %>
    <%= f.input :street_1 %>
    <%= f.input :street_2 %>
    <%= f.input :city %>
    <%= f.input :state %>
    <%= f.input :zip %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
