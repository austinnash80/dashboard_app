<p id="notice"><%= notice %></p>

<h1 class="text-center">Id Number Storages</h1>

<p>Sequoia members order: <%= IdNumberStorage.pluck(:sequoia_members_order_id)[0] %></p>
<p>Daily Sales Id: <%= IdNumberStorage.pluck(:daily_sales_id)[0] %></p>

<div class="m-5">
  <table class="table">
    <thead>
      <tr>
        <th>Sequoia members order</th>
        <th>Daily Sales Id</th>
        <th>Daily Sales Sequoia Customer Order Id</th>
        <th colspan="3"></th>
      </tr>
    </thead>

    <tbody>
      <% @id_number_storages.order(daily_sales_sequoia_customer_order_id: :DESC).limit(100).each do |id_number_storage| %>
        <tr>
          <!-- <td><%= id_number_storage.sequoia_members_order_id %></td> -->
          <!-- <td><%= id_number_storage.daily_sales_id %></td> -->
          <td><%= id_number_storage.daily_sales_sequoia_customer_order_id %></td>
          <td><%= link_to 'Show', id_number_storage %></td>
          <td><%= link_to 'Edit', edit_id_number_storage_path(id_number_storage) %></td>
          <!-- <td><%= link_to 'Destroy', id_number_storage, method: :delete, data: { confirm: 'Are you sure?' } %></td> -->
        </tr>
      <% end %>
    </tbody>
  </table>
</div>
