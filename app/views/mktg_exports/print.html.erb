
<div class="mktg_print_page_top">
  <% MktgExport.order(exp: :asc).order(uid: :asc).all.each do |i| %>
    <p class="mktg_print_padding mktg_export_text"><%= i.fname.present? ? i.fname.titlecase : ""%> <%= i.lname.present? ? i.lname.titlecase : ''%><br> <%= i.street_1.present? ? i.street_1.titlecase : '' %> <%= i.street_2.present? ? i.street_2.titlecase : ''%> <br> <%= i.city.present? ? i.city.titlecase : ''%> <%= i.state.present? ? i.state.upcase : ''%> <%= i.zip %></p>
  <% end %>
</div>
