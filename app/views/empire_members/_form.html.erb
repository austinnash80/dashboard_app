
<%= simple_form_for(@empire_member) do |f| %>
  <%= f.error_notification %>
  <%= f.error_notification message: f.object.errors[:base].to_sentence if f.object.errors[:base].present? %>

  <div class="form-inputs">
    <%= f.input :uid %>
    <%= f.input :lname %>
    <%= f.input :first_purchase, include_blank: true %>
    <%= f.input :last_purchase, include_blank: true %>
    <%= f.input :state %>
    <%= f.input :purchases %>
  </div>
<br>
  <div class="form-actions">
    <%= f.button :submit, 'Submit', class: 'btn btn-primary btn-gradiant w100' %>
  </div>
<br>
<% end %>
