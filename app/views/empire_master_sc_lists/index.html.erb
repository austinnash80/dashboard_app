<p id="notice"><%= notice %></p>

<h1 class="text-center">Empire Master SC List</h1>

    <p class="fs-4">Upload New List</p>
      <%= form_tag import_empire_master_sc_lists_path, multipart: true do %>
      <%= file_field_tag :file, class: 'btn btn-sm btn-light' %>
      <%= submit_tag 'Upload', class: 'btn btn-sm btn-primary text-light' %>
    <% end %>

    <% if params['remove_all'] == 'yes' %>
      <p><%= link_to "Confirm Delete", empire_master_sc_lists_path(remove_all: 'yes', confirm: 'yes'), class: 'btn btn-sm btn-danger mt-4' %></p>
    <% else %>
      <p><%= link_to "Remove All Records", empire_master_sc_lists_path(remove_all: 'yes'), class: 'btn btn-sm btn-warning mt-4' %></p>
    <% end %>

    <p><%= link_to 'Export List (csv)', empire_master_sc_lists_path('csv'), class: 'btn btn-sm btn-secondary text-light mt-2' %></p>

<div class="m-5">
<table class="table table-striped">
  <thead>
    <caption class="caption-top">Total Records: <%= number_with_delimiter(EmpireMasterScList.count) %> | Display Limit: 250</caption>
    <tr class="bg-secondary bg-gradient text-light">
      <th>Lid</th>
      <th>List</th>
      <th>Lic state</th>
      <th>Lic</th>
      <th>Record type</th>
      <th>Lic status</th>
      <th>Iss date s</th>
      <th>Iss date</th>
      <th>Exp date s</th>
      <th>Exp date</th>
      <th>Fname</th>
      <th>Mi</th>
      <th>Lname</th>
      <th>Suf</th>
      <th>Co</th>
      <th>Add</th>
      <th>Add2</th>
      <th>City</th>
      <th>St</th>
      <th>Zip</th>
      <th>Email</th>
      <th>Phone</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <% @empire_master_sc_lists.limit(250).each do |empire_master_sc_list| %>
      <tr>
        <td><%= empire_master_sc_list.lid %></td>
        <td><%= empire_master_sc_list.list %></td>
        <td><%= empire_master_sc_list.lic_state %></td>
        <td><%= empire_master_sc_list.lic %></td>
        <td><%= empire_master_sc_list.record_type %></td>
        <td><%= empire_master_sc_list.lic_status %></td>
        <td><%= empire_master_sc_list.iss_date_s %></td>
        <td><%= empire_master_sc_list.iss_date %></td>
        <td><%= empire_master_sc_list.exp_date_s %></td>
        <td><%= empire_master_sc_list.exp_date %></td>
        <td><%= empire_master_sc_list.fname %></td>
        <td><%= empire_master_sc_list.mi %></td>
        <td><%= empire_master_sc_list.lname %></td>
        <td><%= empire_master_sc_list.suf %></td>
        <td><%= empire_master_sc_list.co %></td>
        <td><%= empire_master_sc_list.add %></td>
        <td><%= empire_master_sc_list.add2 %></td>
        <td><%= empire_master_sc_list.city %></td>
        <td><%= empire_master_sc_list.st %></td>
        <td><%= empire_master_sc_list.zip %></td>
        <td><%= empire_master_sc_list.email %></td>
        <td><%= empire_master_sc_list.phone %></td>
        <td><%= link_to 'Show', empire_master_sc_list %></td>
        <td><%= link_to 'Edit', edit_empire_master_sc_list_path(empire_master_sc_list) %></td>
        <td><%= link_to 'Destroy', empire_master_sc_list, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
</div>
